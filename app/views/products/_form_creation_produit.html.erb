<div class="contenaire">
  <div class="col-xs12 col-md-offset-3 col-md-6">
    <%= simple_form_for(@product) do |f| %>
      <%= f.error_notification %>

      <%= f.input :name %>
      <%= f.input :description %>
      <%= f.input :address %>
      <%= f.input :city %>
      <%= f.input :country, priority: [ "France" ], collection: [ "Andorra", "Austria", "Belgium", "Croatia", "Bulgaria", "Denmark", "France", "Finland", "Germany", "Hungary", "Italy", "Ireland", "Liechtenstein", "Luxembourg", "Poland", "Portugal", "Slovenia", "Slovakia", "Spain", "Ukraine", "United Kingdom"] %>
      <%= f.input :price %>
      <%= f.input :begin_date %>
      <%= f.input :end_date %>
      <%= f.input :picture %>
      <%= f.input :picture_cache, as: :hidden %>
      <%= f.button :submit %>
    <% end %>
  </div>
</div>
